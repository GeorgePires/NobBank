---
openapi: 3.0.1
info:
  title: API V1
  version: v1
paths:
  "/api/v1/accounts":
    get:
      summary: list accounts
      responses:
        '200':
          description: successful
  "/api/v1/accounts/deposit":
    post:
      summary: deposit account
      responses:
        '200':
          description: successful
  "/api/v1/accounts/withdraw":
    post:
      summary: withdraw account
      responses:
        '200':
          description: successful
servers:
- url: https://{defaultHost}
  variables:
    defaultHost:
      default: www.example.com
